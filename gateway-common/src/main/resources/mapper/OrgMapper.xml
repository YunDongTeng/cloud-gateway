<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cloud.gateway.mapper.SysOrgDao">

    <insert id="save" parameterType="com.cloud.gateway.entity.Org">
        insert into org(id,name,code,parent_id) values (#{id},#{name},#{code},#{parentId})
    </insert>
</mapper>